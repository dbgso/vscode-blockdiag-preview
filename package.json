{
	"name": "vscode-blockdiag-preview",
	"displayName": "Preview Blockdiag ",
	"description": "Live preview for Blockdiag",
	"version": "0.0.10",
	"publisher": "dbgso",
	"galleryBanner": {
		"color": "#5c2d91",
		"theme": "dark"
	},
	"bugs": {
		"url": "https://github.com/Microsoft/vscode-extension-samples/issues",
		"email": "egamma@microsoft.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples.git"
	},
	"homepage": "https://github.com/Microsoft/vscode-extension-samples/tree/master/textdocumentprovider-sample/README.md",
	"categories": [
		"Other"
	],
	"engines": {
		"vscode": "^0.10.7"
	},
	"activationEvents": [
		"onCommand:extension.showBlockDiagPreview"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.showBlockDiagPreview",
				"title": "Show BlockDiag Preview"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "extension.showBlockDiagPreview",
					"when": "resourceLangId == css"
				}
			]
		},
		"languages": [
			{
				"id": "blockdiagram",
				"aliases": [
					"BlockDiag"
				],
				"extensions": [
					".diag"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"snippets": [
			{
				"language": "blockdiagram",
				"path": "./snippets/diagram.json"
			}
		],
		"keybindings": [
			{
				"command": "extension.showBlockDiagPreview",
				"key": "Alt + D"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"tslint": "tslint -c tslint.json src/extension.ts"
	},
	"devDependencies": {
		"typescript": "^2.1.4",
		"vscode": "^1.1.17",
		"@types/node": "*"
	},
	"dependencies": {
		"request": "^2.81.0"
	}
}
